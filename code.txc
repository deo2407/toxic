// declaring a variable
bandz cash = 51456.99
bandz 10k = 10000

// function declaration
scheme add() {
    cash = cash + 10k
}

// calling functions
add()

// conditionals
fr cash > 50000 {
    spill("loaded")
} cap {
    spill("broke")
}

// loops
spin 5 {
    cash = cash + 10k
    spill(cash)
}

