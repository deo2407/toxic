// declaring a variable
stash racks = 51456.99
stash 10k = 10000

// function declaration
scheme add() {
    racks = racks + 10k
}

// calling functions
add()

// conditionals
fr racks > 50000 {
    spill("loaded")
} cap {
    spill("broke")
}

// loops
spin 5 {
    racks = racks + 10k
    spill(racks)
}

